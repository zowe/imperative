    "yargs-parser": "^20.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "transform": {
      ".(ts)": "ts-jest"
    },
    "testRegex": "(test|spec)\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node"
  },
  "jest-html-reporter": {
    "pageTitle": "Sample \"hello world\" integration CLI Tests",
